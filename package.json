  "bugs": {
    "url": "https://github.com/mohamedsamara/mern-ecommerce/issues"
  },
  "homepage": "https://github.com/mohamedsamara/mern-ecommerce#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.25.9",
    "cross-env": "^5.2.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "gradio-client": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
